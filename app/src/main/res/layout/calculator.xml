<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/Theme.AppCompat.Light.NoActionBar">

    <Button
        android:id="@+id/vnum1"
        style="?android:textAppearanceSmall"
        android:textSize="20sp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="?android:attr/colorBackground"
        android:text="1" />

    <Button
        android:id="@+id/vnum2"
        style="?android:textAppearanceSmall"
        android:textSize="20dp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/vnum1"
        android:background="?android:attr/colorBackground"
        android:text="2" />

    <Button
        android:id="@+id/vnum3"
        style="?android:textAppearanceSmall"
        android:textSize="20dp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/vnum2"
        android:background="?android:attr/colorBackground"
        android:text="3" />

    <Button
        android:id="@+id/vnum4"
        style="?android:textAppearanceSmall"
        android:textSize="20dp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum1"
        android:layout_alignEnd="@+id/vnum1"
        android:background="?android:attr/colorBackground"
        android:text="4" />

    <Button
        android:id="@+id/vnum5"
        style="?android:textAppearanceSmall"
        android:textSize="20dp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum1"
        android:layout_toEndOf="@+id/vnum1"
        android:background="?android:attr/colorBackground"
        android:text="5" />

    <Button
        android:id="@+id/vnum6"
        style="?android:textAppearanceSmall"
        android:textSize="20dp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum1"
        android:layout_toEndOf="@+id/vnum2"
        android:background="?android:attr/colorBackground"
        android:text="6" />

    <Button
        android:id="@+id/vnum7"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum4"
        android:layout_alignParentStart="true"
        android:background="?android:attr/colorBackground"
        android:text="7"
        android:textSize="20dp" />

    <Button
        android:id="@+id/vnum8"
        style="?android:textAppearanceSmall"
        android:textSize="20dp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum4"
        android:layout_toEndOf="@+id/vnum1"
        android:background="?android:attr/colorBackground"
        android:text="8" />


    <Button
        android:id="@+id/vnum9"
        style="?android:textAppearanceSmall"
        android:textSize="20dp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum4"
        android:layout_marginStart="150dp"
        android:background="?android:attr/colorBackground"
        android:text="9" />

    <Button
        android:id="@+id/vnum0"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum1"
        android:layout_toEndOf="@+id/vnum3"
        android:background="?android:attr/colorBackground"
        android:text="0"
        android:textSize="20dp" />

    <Button
        android:id="@+id/vmultiplication"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum7"
        android:layout_marginLeft="150dp"
        android:background="?android:attr/colorBackground"
        android:text="*"
        android:textSize="20dp" />

    <Button
        android:id="@+id/vdivide"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum7"
        android:layout_toEndOf="@+id/vnum3"
        android:background="?android:attr/colorBackground"
        android:text="/"
        android:textSize="20dp" />

    <Button
        android:id="@+id/vpuls"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum7"
        android:layout_alignParentStart="true"
        android:background="?android:attr/colorBackground"
        android:text="+"
        android:textSize="20dp" />

    <Button
        android:id="@+id/vsubtraction"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum7"
        android:layout_toEndOf="@+id/vnum1"
        android:background="?android:attr/colorBackground"
        android:text="-"
        android:textSize="20dp" />

    <Button
        android:id="@+id/vequal"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/vnum3"
        android:background="?android:attr/colorBackground"
        android:text="="
        android:textSize="20dp" />

    <Button
        android:id="@+id/vdot"
        style="?android:textAppearanceSmall"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum4"
        android:layout_toEndOf="@+id/vnum3"
        android:background="?android:attr/colorBackground"
        android:text="."
        android:textSize="20dp" />


    <TextView
        android:id="@+id/vinput"
        android:layout_width="match_parent"
        android:layout_height="266dp"
        android:layout_above="@+id/vmultiplication"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="1dp"
        android:background="?android:attr/colorBackground"
        android:text="8+8-9*5/4"
        android:textAppearance="@android:style/TextAppearance.Material.Display1"
        android:textSize="32sp" />

    <Button
        android:id="@+id/vhistory"
        android:layout_width="83dp"
        android:layout_height="75dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_toEndOf="@+id/vnum0"
        android:background="?android:attr/actionModeBackground"
        android:text="History" />

    <Button
        android:id="@+id/vback"
        android:layout_width="83dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum7"
        android:layout_alignParentEnd="true"
        android:background="?android:attr/colorBackground"
        android:text="B" />

    <Button
        android:id="@+id/vclean"
        android:layout_width="83dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum4"
        android:layout_alignParentEnd="true"
        android:background="?android:attr/colorBackground"
        android:text="C" />

    <Button
        android:id="@+id/vroute"
        android:layout_width="83dp"
        android:layout_height="75dp"
        android:layout_above="@+id/vnum1"
        android:layout_marginStart="1dp"
        android:layout_toEndOf="@+id/vnum0"
        android:background="?android:attr/colorBackground"
        android:text="Route" />
</RelativeLayout>